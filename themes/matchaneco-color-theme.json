	"tokenColors": [
		//
		// 言語共通
		//
		{
			"scope": [
				"source punctuation.definition.comment", // コメントの開始・終了文字
				"source comment" // コメント
			],
			"settings": {
				"foreground": "#6e9759",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source punctuation.definition.string", // 文字列の開始・終了文字
				"source string.quoted", // 文字列
				"source string", // 文字列
				"text.html string", // 文字列
			],
			"settings": {
				"foreground": "#a2de82",
				"fontStyle": ""
			}
		},
		//
		// html 用
		//
		{
			"scope": [
				"punctuation.definition.tag.begin.html", // 開始タグの開始文字
				"punctuation.definition.tag.end.html", // 開始タグの終了文字
				"punctuation.separator.key-value.html", // 属性の区切り文字
				"entity.other.attribute-name.html", // 属性名
				"entity.name.tag.html" // タグ名
			],
			"settings": {
				"foreground": "#babb75",
				"fontStyle": ""
			}
		},
		//
		// PHP 用
		//
		{
			"scope": [
				"text.html.php punctuation.section.embedded.begin.php", // 定数
				"text.html.php punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#D1DED3",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.php constant.numeric", // 定数
				"source.php constant.language", // 定数
				"source.php storage.type", // 型
				"source.php string.regexp", // 正規表現
				"source.php keyword.other.new", // new
				"source.php keyword.operator.regexp", // 正規表現の演算子
				"source.php entity.other.alias", // エイリアス
			],
			"settings": {
				"foreground": "#ffb477",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.php keyword.control.import", // PHPの組み込み関数
				"source.php support", // PHPの組み込み関数
				"source.php entity.name", // 関数名
			],
			"settings": {
				"foreground": "#e2e38b",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.php support.other", // 変数
			],
			"settings": {
				"foreground": "#babb75",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.php variable", // 変数
			],
			"settings": {
				"foreground": "#afe4f5",
				"fontStyle": ""
			}
		},
		//
		// javascript 用
		//
		{
			"scope": [
				"source.js constant.numeric", // 定数
				"source.js constant.language", // 定数
				"source.js storage.type", // 型
				"source.js string.regexp", // 正規表現
				"source.js keyword.operator.new", // new
				"source.js keyword.operator.regexp", // 正規表現の演算子
				"source.js entity.other.alias", // エイリアス
			],
			"settings": {
				"foreground": "#ffb477",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.js keyword.control.import", // PHPの組み込み関数
				"source.js support", // PHPの組み込み関数
				"source.js entity.name", // 関数名
			],
			"settings": {
				"foreground": "#e2e38b",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.js support.other", // 変数
			],
			"settings": {
				"foreground": "#babb75",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.js variable", // 変数
			],
			"settings": {
				"foreground": "#afe4f5",
				"fontStyle": ""
			}
		},
		//
		// css 用
		//
		{
			"scope": [
				"source.css keyword.operator.regexp", // 正規表現の演算子
				"keyword.other.unit.px.css",   // タグ名
			],
			"settings": {
				"foreground": "#ffb477",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.css entity.name.tag",   // タグ名
				"source.css entity",   // タグ名
			],
			"settings": {
				"foreground": "#e2e38b",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.css variable", // 変数
			],
			"settings": {
				"foreground": "#afe4f5",
				"fontStyle": ""
			}
		},
		//
		// jsonc 用
		//
		{
			"scope": [
				"source.json constant.numeric", // 定数
				"source.json constant.language", // 定数
				"source.json storage.type", // 型
				"source.json string.regexp", // 正規表現
				"source.json keyword.operator.new", // new
				"source.json keyword.operator.regexp", // 正規表現の演算子
				"source.json entity.other.alias", // エイリアス
			],
			"settings": {
				"foreground": "#ffb477",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"source.json keyword.control.import", // PHPの組み込み関数
				"source.json support", // PHPの組み込み関数
				"source.json entity.name", // 関数名
			],
			"settings": {
				"foreground": "#e2e38b",
				"fontStyle": ""
			}
		}
